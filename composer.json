{
  "name": "chuk-shirley/laminas-no-intl",
  "description": "Laminas MVC module for php installations with no internationalization extension",
  "license": "MIT",
  "keywords": [
    "intl",
    "internationalization",
    "laminas"
  ],
  "require": {
    "php": "^7.0",
    "laminas/laminas-i18n": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "laminas/laminas-servicemanager": "^3.4",
    "laminas/laminas-modulemanager": "^2.8",
    "laminas/laminas-view": "^2.11"
  },
  "autoload": {
    "psr-4": {
      "NoIntl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NoIntlIntegrationTest\\": "tests/integration",
      "NoIntlUnitTest\\": "tests/unit"
    }
  }
}
